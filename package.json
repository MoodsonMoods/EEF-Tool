{
  "name": "eef-tool-2.0",
  "version": "1.0.0",
  "description": "ESPN Eredivisie Fantasy (EEF) Toolkit - Advanced analytics and planning tool",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "ingest": "node scripts/ingest.js",
    "normalize": "node scripts/normalize.js",
    "generate-types": "node scripts/generate-types.js",
    "scrape-fbref-2526": "node scripts/fetch-fbref-puppeteer.js --season=2025-2026",
    "update-fdr-from-scrape": "node scripts/update-fdr-calculator.js",
    "fetch-fbref-players": "node scripts/fetch-fbref-player-stats.js",
    "fetch-fbref-players-direct": "node scripts/fetch-fbref-players-direct.js",
    "scrape-and-update": "npm run scrape-fbref-2526 && npm run update-fdr-from-scrape",
    "update-data": "npm run ingest && npm run normalize && npm run generate-types && npm run fetch-fbref-players-direct && npm run scrape-and-update",
    "deploy": "npm run build && vercel --prod"
  },
  "dependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.0.0",
    "json-schema-to-typescript": "^2.0.0",
    "next": "^15.0.0",
    "postcss": "^8.5.6",
    "puppeteer": "^24.14.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.0.0",
    "zustand": "^4.5.0"
  },
  "keywords": [
    "fantasy-football",
    "eredivisie",
    "analytics",
    "planning"
  ],
  "author": "Joaquim Moody",
  "license": "MIT"
}
